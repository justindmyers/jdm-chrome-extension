{
    "manifest_version": 2,
    "name": "Triangles Background",
    "description": "A configurable start page",
    "version": "1.0",
    "permissions": [
        "activeTab",
        "https://ajax.googleapis.com/",
        "history",
        "topSites",
        "geolocation"
    ],
    "chrome_url_overrides": {
        "newtab": "dist/index.html"
    }
}